}

QComboBox:hover {
    border: 2px solid rgb(220, 149, 196);
}

QComboBox:focus {
    border: 2px solid #d484b4; /* Borde al enfocar */
}

QComboBox::drop-down {
    width: 20px; /* Ancho del botón desplegable */
