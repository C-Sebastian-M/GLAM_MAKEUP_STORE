}

QComboBox::down-arrow {
    image: url(drop_down_icon.png); /* Cambiar el icono de flecha */
}

QComboBox::down-arrow:hover {
    image: url(drop_down_icon_hover.png); /* Cambiar el icono de flecha al pasar el cursor */
